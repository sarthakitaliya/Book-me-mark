{
  "manifest_version": 3,
  "name": "Savvio",
  "version": "1.0.0",
  "description": "Save bookmarks from any tab",
  "action": {
    "default_popup": "index.html"
  },
  "background": {
    "service_worker": "src/background.ts"
  },
  "permissions": ["tabs", "storage", "activeTab"],
  "host_permissions": ["<all_urls>"]
}